        {% block stylesheets %}{% endblock %}
        <script type="text/javascript">
            app = {
                api: {
                    endpoints: {
                        tournamenttypeaction: "{{path('api_tournament_type_action', {id:'ID'})|raw}}"
                    },
                    path: function(endpoint, params) {
                        var path = null;
                        if (this.endpoints.hasOwnProperty(endpoint)) {
                            path = this.endpoints[endpoint];
                            for (var item in params) {
                                path = path.replace(item, params[item])
                            }
                        }
                        return path;
                    }
                }
            }
        </script>
    </head>
